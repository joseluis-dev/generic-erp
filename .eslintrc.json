{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["next/core-web-vitals","standard-with-typescript"],
  "plugins": ["@typescript-eslint"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/no-misused-promises": [
      "error",
      {
        "checksVoidReturn": {
          "arguments": false,
          "attributes": false
        }
      }
    ]
  }
}
